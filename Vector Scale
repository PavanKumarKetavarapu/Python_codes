n=float(input("Enter the Value: "))
if n<2.0:
    print("Micro")
elif n>=2.0 and n<3.0:
    print("Very Minor")
elif n>=3.0 and n<4.0:
    print("Minor")
elif n>=4.0 and n<5.0:
    print("Light")
elif n>=5.0 and n<6.0:
    print("Moderate")
elif n>=6.0 and n<7.0:
    print("Strong")
elif n>=7.0 and n<8.0:
    print("Major")
elif n>=8.0 and n<10.0:
    print("Great")
elif n>10:
    print("Meteoric")
else:
    print("you entered negative value")
